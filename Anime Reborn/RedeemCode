--1
local args = {
	[1] = "Codes/RedeemCode",
	[2] = "10MVisits"
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("UiCommunication"):FireServer(unpack(args))
wait(2)

