	end
	tab(635 , 375)------Tab hiện bảng update
	wait(1)
	for i =1 ,25 do
		wait(4)

		if btnUpgrade.Text == "6,000" then
			break
		else
			ugradeUnit( 635 , 375 )
		end
	end



	for i =1 ,50 do
		wait(5)
		ugradeUnit( 410 , 155 )
		wait(5)
		ugradeUnit( 410 , 210 )
		wait(5)
		ugradeUnit( 410 , 265 )
		wait(5)
		ugradeUnit( 410 , 320 )

		wait(5)
		ugradeUnit( 720 , 155 )
		wait(5)
		ugradeUnit( 635 , 210 )
		wait(5)
		ugradeUnit( 635 , 265 )
		wait(5)
		ugradeUnit( 635 , 320 )
		wait(5)
		ugradeUnit( 635 , 375 )
	end

end
