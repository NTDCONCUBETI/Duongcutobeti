	function SpawmTroop (i,x,y)
		while true do
			camera.CFrame = cameraCFrame * rotation
			wait(1)
			click( tableUnit[i] )
			wait(1)
			tab( x , y )
			wait(1)
			presskey(119)
			presskey(115)
			wait(1)
			presskey(101)
			tab( x , y )
			wait(1)
			if #game.Workspace.Units:GetChildren() > UnitsInWS then
				UnitsInWS = #game.Workspace.Units:GetChildren()
				break
			end
		end
	end



	function ugradeUnit(x,y)
		camera.CFrame = cameraCFrame * rotation
		wait(0.5)
		tab( x , y )
		wait(1)
		click(btnUpgrade)
	end

	spawn(function()
		pcall(function()
