	tpmap_nomal={
		CFrame.new(-182.66664123535156, 11.36839771270752, -283.6666564941406),
		CFrame.new(-210.93682861328125, 11.368400573730469, -255.42739868164062),
		CFrame.new(-240.77003479003906, 11.368404388427734, -225.57411193847656),
