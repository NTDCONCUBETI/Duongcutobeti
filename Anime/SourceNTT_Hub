	name1.TextXAlignment = Enum.TextXAlignment.Left

	name2 = Instance.new("TextButton")
	name2.Parent = name1
	name2.BackgroundColor3 = Color3.fromRGB(250,250,250)
	name2.Position = UDim2.new(0.80000000, 0, 0, 0)
	name2.Size = UDim2.new(0, 65, 0, 20)
