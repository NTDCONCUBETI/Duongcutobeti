	make_mainscrollingframe(b_page4, main, false)


	make_mainbutton(b_1, bar1, "   Main", UDim2.new(0, 0, 0.000000000, 0))
	make_mainbutton(b_2, bar1,"   Units", UDim2.new(0, 0, 0.050000000, 0))

	toolbar=game:GetService("Players").LocalPlayer.PlayerGui.HUD.Toolbar.UnitBar.UnitHolder

	function check()
		c_unit=#game:GetService("Workspace").Units:GetChildren()
		if c_unit == 0 then
			cfmob=_G.cf_unit1
			smob=tonumber(u_b1.Text)+1
			vmob=_G.vector3_unit1

		elseif c_unit == 1 then
			cfmob=_G.cf_unit2
			smob=tonumber(u_b3.Text)+1
			vmob=_G.vector3_unit2

		elseif c_unit == 2 then
			cfmob=_G.cf_unit3
			vmob=_G.vector3_unit3
			smob=tonumber(u_b5.Text)+1

		elseif c_unit == 3 then
			cfmob=_G.cf_unit4
			vmob=_G.vector3_unit4
			smob=tonumber(u_b7.Text)+1

		elseif c_unit == 4 then
			cfmob=_G.cf_unit5
			vmob=_G.vector3_unit5
			smob=tonumber(u_b9.Text)+1

		elseif c_unit == 5 then
			cfmob=_G.cf_unit6
			vmob=_G.vector3_unit6
			smob=tonumber(u_b11.Text)+1

		elseif c_unit == 6 then

		end end

	button_=game:GetService("Players").LocalPlayer.PlayerGui.HUD.LocalUnitHolder.MainFrame.HolderButtons.UpgradeButton
	unithoder=game:GetService("Players").LocalPlayer.PlayerGui.HUD.Toolbar.UnitBar.UnitHolder:GetChildren()
	waveframe=game:GetService("Players").LocalPlayer.PlayerGui.WaveTopBar.Wave

	if game:GetService("Players").LocalPlayer.PlayerGui.HUD:FindFirstChild("LocalUnitHolder") then

	else
		game:GetService("Players").LocalPlayer.PlayerGui.UI.GUIs:GetChildren()[1].Parent=game:GetService("Players").LocalPlayer.PlayerGui.HUD
