	}

	spawn(function()
		while task.wait() do       
			pcall(function()
				if _G.m_b1 then
					checkmap()
					ii=tonumber(m_b.Text)+2
					if machpage.Visible == false then
						p(tpmap_nomal[math.random(1,6)])
					end

					if machpage.Visible == true then
						if machpage.Frame.Main.LeftSide.Visible==false then
							posmou=machpage.WorldSelectFrame.ScrollingFrame:GetChildren()[_G.i].ImageButton
							posmouse_gui(posmou, 0, 20)
						end end

					if machpage.Frame.Main.LeftSide.Visible==true then
						wait(0.5)
						if machpage.Frame.OptionsHolder:GetChildren()[2].ConfirmButton.Visible == false then
							posmou1=machpage.Frame.Main.LeftSide.ScrollingFrame:GetChildren()[ii].ChapterButton
							posmouse_gui(posmou1, 0, 20)
						end
						wait(0.5)
						if machpage.Frame.OptionsHolder:GetChildren()[2].ConfirmButton.Visible == true then
							posmou1=machpage.Frame.Main.LeftSide.ScrollingFrame:GetChildren()[ii].ChapterButton
							posmouse_gui(posmou1, 0, 20)
						end
						wait(0.5)
						if machpage.Frame.OptionsHolder:GetChildren()[2].ConfirmButton.Visible == true then
							posmou2=machpage.Frame.OptionsHolder:GetChildren()[2].ConfirmButton
							posmouse_gui(posmou2, 0, 20)
						end  end
					wait(0.5)
					if game:GetService("Players").LocalPlayer.PlayerGui.HUD.MatchDisplayHolder.MatchDisplayFrame.OptionsHolder.StartButtonHolder.ConfirmButton.Visible == true then
						posmou4=game:GetService("Players").LocalPlayer.PlayerGui.HUD.MatchDisplayHolder.MatchDisplayFrame.OptionsHolder.StartButtonHolder.ConfirmButton
						posmouse_gui(posmou4, 0, 20)
					end
				end end) end end)

	make_select1(m_f2, m_b2, b_page1, "Map : Slect", UDim2.new(0, 0, 0.1000000000, 0))
	m_b2.MouseButton1Down:connect(function()
		m_tbar.Visible = true
	end) 

	function mapbar()
		make_selectscrollingframe(m_tbar, b_page1, UDim2.new(0, 0, 0.1700000, 0))
		make_selectbutton(m_ft1, m_bt1, m_b2, m_tbar, "Windmill Vilage", "Map : Windmill Vilage", UDim2.new(0, 0, 0.0000000, 0))
		make_selectbutton(m_ft2, m_bt2, m_b2, m_tbar, "Haunted City", "Map : Haunted City", UDim2.new(0, 0, 0.0500000, 0))
		make_selectbutton(m_ft3, m_bt3,m_b2, m_tbar, "Cursed Academy", "Map : Cursed Academy", UDim2.new(0, 0, 0.1000000, 0))
		make_selectbutton(m_ft4, m_bt4, m_b2, m_tbar, "Blue Planet", "Map : Blue Planet", UDim2.new(0, 0, 0.1500000, 0))
		make_selectbutton(m_ft5, m_bt5, m_b2, m_tbar, "Underwater Temple", "Map : Underwater Temple", UDim2.new(0, 0, 0.200000, 0))
		make_selectbutton(m_ft6, m_bt6, m_b2, m_tbar, "Swordman Dojo", "Map : Swordman Dojo", UDim2.new(0, 0, 0.2500000, 0))
		make_selectbutton(m_ft7, m_bt7, m_b2, m_tbar, "Snowy Woods", "Map : Snowy Woods", UDim2.new(0, 0, 0.3000000, 0))
		make_selectbutton(m_ft8, m_bt8, m_b2, m_tbar, "Crystal Cave", "Map : Crystal Cave", UDim2.new(0, 0, 0.35000000, 0))
	end

	function vmap(x)
		machpage.WorldSelectFrame.ScrollingFrame:GetChildren()[3].ImageButton.Visible=false
		machpage.WorldSelectFrame.ScrollingFrame:GetChildren()[4].ImageButton.Visible=false
		machpage.WorldSelectFrame.ScrollingFrame:GetChildren()[5].ImageButton.Visible=false
		machpage.WorldSelectFrame.ScrollingFrame:GetChildren()[6].ImageButton.Visible=false
		machpage.WorldSelectFrame.ScrollingFrame:GetChildren()[7].ImageButton.Visible=false
		machpage.WorldSelectFrame.ScrollingFrame:GetChildren()[8].ImageButton.Visible=false
		machpage.WorldSelectFrame.ScrollingFrame:GetChildren()[9].ImageButton.Visible=false
		machpage.WorldSelectFrame.ScrollingFrame:GetChildren()[10].ImageButton.Visible=false
		machpage.WorldSelectFrame.ScrollingFrame:GetChildren()[x].ImageButton.Visible=true
	end

	function checkmap()
		if m_b2.Text == "Map : Windmill Vilage" then
			_G.i=9
			vmap(9)
		elseif m_b2.Text == "Map : Haunted City" then
			_G.i=8
			vmap(8)
		elseif m_b2.Text == "Map : Cursed Academy" then
			_G.i=3
			vmap(3)
		elseif m_b2.Text == "Map : Blue Planet" then
			_G.i=5
			vmap(5)
		elseif m_b2.Text == "Map : Underwater Temple" then
			_G.i=4
			vmap(4)
		elseif m_b2.Text == "Map : Swordman Dojo" then
			_G.i=7
			vmap(7)
		elseif m_b2.Text == "Map : Snowy Woods" then
			_G.i=6
			vmap(6)
		elseif m_b2.Text == "Map : Crystal Cave" then
			_G.i=10
			vmap(10)
		end end

	make_buttonnumber(m_f,m_b,b_page1,"   Slect Chapter :" ,"1",UDim2.new(0, 0, 0.1700000000, 0),UDim2.new(0.17000000, 0, 0, 0))
	m_b.TextSize = 14.000
	m_b.MouseButton1Down:connect(function()
		if m_b.Text == "7" then
			m_b.Text = "1"
		else
			m_b.Text = m_b.Text+1
		end end)

	mapbar()

	function keyboard(x)
		game:GetService("VirtualInputManager"):SendKeyEvent(true,x,false,game)
		game:GetService("VirtualInputManager"):SendKeyEvent(false,x,false,game)      
	end

	make_together(s_f1, s_b1, b_page2, "   Auto Buy Unit X1", UDim2.new(0, 0, 0.03000000, 0))
	s_b1.MouseButton1Down:connect(function()
		--on off 
		if s_b1.Text == "" then --on
			s_b1.Text = "X"
			_G.s_b1 = true 
		elseif s_b1.Text == "X" then --off
			s_b1.Text = ""
			_G.s_b1 = false
		end
	end)  

	spawn(function()
		while task.wait() do       
			pcall(function()
				if _G.s_b1 then
					if pages_.SummonPage.Visible==false then
						p(CFrame.new(-221.64825439453125, 15.862335205078125, -470.12689208984375))
						keyboard("E")
						VirtualInputManager:SendMouseButtonEvent(0, 0, 0, true, game, 0)
						wait(0.1)
						VirtualInputManager:SendMouseButtonEvent(0, 0, 0, false, game, 0)
					else
						posmouse_gui(pages_.SummonPage.Holder.HolderBuySummon.One,0,30)
						wait(0.5)
					end end end) end end)

	make_together(s_f2, s_b2, b_page2, "   Auto Buy Unit X10", UDim2.new(0, 0, 0.1000000, 0))
	s_b2.MouseButton1Down:connect(function()
		--on off 
		if s_b2.Text == "" then --on
			s_b2.Text = "X"
			_G.s_b2 = true 
		elseif s_b2.Text == "X" then --off
			s_b2.Text = ""
			_G.s_b2 = false
		end
	end)  

	spawn(function()
		while task.wait() do       
			pcall(function()
				if _G.s_b2 then
					if pages_.SummonPage.Visible==false then
						p(CFrame.new(-221.64825439453125, 15.862335205078125, -470.12689208984375))
						keyboard("E")
						VirtualInputManager:SendMouseButtonEvent(0, 0, 0, true, game, 0)
						wait(0.1)
						VirtualInputManager:SendMouseButtonEvent(0, 0, 0, false, game, 0)
					else
						posmouse_gui(pages_.SummonPage.Holder.HolderBuySummon.Ten,0,30)
						wait(0.5)
					end end end) end end)

	function b_page(name1,name2)
		b_page1.Visible = false
		b_page2.Visible = false
		b_page3.Visible = false
		b_page4.Visible = false
		name1.Visible=true
		TextLabel.Text=name2
	end

	b_1.MouseButton1Down:connect(function()
		b_page(b_page1, ("NTT HUB | Main | ".._G.linkdis))
	end)

	b_2.MouseButton1Down:connect(function()
		b_page(b_page2, ("NTT HUB | Shop | ".._G.linkdis))
	end)

