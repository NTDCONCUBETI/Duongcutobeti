	name1.BorderColor3 = Color3.fromRGB(250, 250, 250)
	name1.Active = true
	name1.Draggable = true
	name1.Visible = false

	name2.Parent = name1
	name2.BackgroundColor3 = Color3.fromRGB(5, 5, 5)
