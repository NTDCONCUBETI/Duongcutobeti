	name1.Size = UDim2.new(0, 498, 0, 30)
	name1.BorderColor3 = Color3.fromRGB(250, 250, 250)
	name1.Font = Enum.Font.Ubuntu
	name1.Text = text
	name1.TextColor3 = Color3.fromRGB(255, 255, 255)
	name1.TextSize = 14.000
	name1.TextWrapped = true

end

